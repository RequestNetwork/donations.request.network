{
  "name": "request-donations",
  "version": "0.3.0",
  "description": "Request Donations Project",
  "license": "MIT",
  "author": "Adam Dowson <adamdowson@googlemail.com>",
  "main": "dist",
  "scripts": {
    "build": "gulp  && babel src -s -D -d dist --presets es2015,stage-0",
    "build:landing": "webpack",
    "dev": "gulp | nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "prestart": "gulp | npm run -s build",
    "start": "gulp | node dist",
    "start:landing": "webpack-dev-server --open --hot --inline",
    "test": "eslint src",
    "watch:landing": "webpack --watch"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.3.1",
    "aws-sdk": "^2.342.0",
    "body-parser": "^1.13.3",
    "bootstrap": "4.1.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "data-store": "^3.1.0",
    "ejs": "^1.0.0",
    "express": "^4.13.3",
    "http-status-codes": "^1.3.0",
    "jquery": "3.3.1",
    "jquery.easing": "^1.4.1",
    "simple-line-icons": "2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.0.20",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "browser-sync": "2.24.7",
    "css-loader": "^1.0.0",
    "eslint": "^3.1.1",
    "file-loader": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "latest",
    "gulp-clean-css": "3.10.0",
    "gulp-csso": "^2.0.0",
    "gulp-header": "2.0.5",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "4.0.1",
    "gulp-uglify": "^2.0.0",
    "nodemon": "^1.9.2",
    "popper.js": "^1.14.4",
    "postcss-loader": "^3.0.0",
    "run-sequence": "^1.2.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
